{
  "name": "@manufac-analytics/inchi",
  "version": "1.0.0-alpha.22",
  "description": "InChI for NodeJS/TypeScript",
  "keywords": [
    "InChI",
    "InChIKey",
    "IUPAC",
    "NIST",
    "Line Notation",
    "Chemistry",
    "NodeJS",
    "TypeScript"
  ],
  "type": "commonjs",
  "exports": "./dist/tsc/src/index.js",
  "main": "dist/tsc/src/index.js",
  "types": "dist/types/src/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/manufac-analytics/inchi.git"
  },
  "bugs": {
    "url": "https://github.com/manufac-analytics/inchi/issues"
  },
  "homepage": "https://github.com/manufac-analytics/inchi#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "refresh": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "tsc",
    "postbuild": "cp src/libinchi.so.1.06.00 dist/tsc/src",
    "serve": "node dist/tsc/src/server.js",
    "test": "node dist/tsc/tests/examples.test.js",
    "pretty": "prettier --write .",
    "preversion": "yarn build && yarn test",
    "postversion": "git push origin v$npm_package_version && git push && yarn publish --new-version $npm_package_version"
  },
  "dependencies": {
    "enum": "^3.0.4",
    "ffi-napi": "^4.0.3",
    "jayson": "^3.6.3",
    "ref-array-di": "^1.2.2",
    "ref-napi": "^3.0.3",
    "ref-struct-di": "^1.1.1"
  },
  "devDependencies": {
    "@types/ffi-napi": "^4.0.2",
    "@types/ref-array-di": "^1.2.2",
    "prettier": "^2.3.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  }
}
