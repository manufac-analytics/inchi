{
  "name": "@manufac-analytics/inchi",
  "version": "1.0.0-alpha.17",
  "description": "InChI for NodeJS/TypeScript",
  "keywords": [
    "InChI",
    "InChIKey",
    "IUPAC",
    "NIST",
    "Line Notation",
    "Chemistry",
    "NodeJS",
    "TypeScript"
  ],
  "type": "commonjs",
  "exports": "./dist/tsc/index.js",
  "main": "dist/tsc/index.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/manufac-analytics/inchi.git"
  },
  "bugs": {
    "url": "https://github.com/manufac-analytics/inchi/issues"
  },
  "homepage": "https://github.com/manufac-analytics/inchi#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "postbuild": "cp src/libinchi.so.1.06.00 dist/tsc/",
    "serve": "node dist/tsc/server.js",
    "test": "node dist/tsc/examples.test.js",
    "pretty": "prettier --write .",
    "preversion": "yarn build && yarn test",
    "postversion": "git push origin v$npm_package_version && git push && yarn publish --new-version $npm_package_version"
  },
  "dependencies": {
    "ffi-napi": "^4.0.3",
    "jayson": "^3.6.3"
  },
  "devDependencies": {
    "@types/ffi-napi": "^4.0.1",
    "prettier": "^2.3.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  }
}
